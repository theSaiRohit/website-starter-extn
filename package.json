{
  "name": "website-starter",
  "displayName": "Website Starter Pack",
  "publisher": "SaiRohitSanniboyina",
  "description": "A website starter pack that creates prefilled html, css and js files with just one click. After installation, open your context menu and click on 'New Website Project...' and input your project name and Voila!!, you're project files are ready.",
  "readme": "README.md",
  "license": "MIT",
  "icon": "/images/extn-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/theSaiRohit/website-starter-extn"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "website-starter-pack.newProjectFromContextMenu",
        "title": "New Website Project..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "website-starter-pack.newProjectFromContextMenu",
          "group": "0_old"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}